  "license": "ISC",
  "devDependencies": {
    "imagemin": "^6.0.0",
    "laravel-mix": "^4.0.8",
    "laravel-mix-copy-watched": "^2.0.0",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0"
  },
  "repository": {
    "type": "git"
  }
}
